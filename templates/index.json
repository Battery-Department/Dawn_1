family: 'Inter', sans-serif;\n            margin: 0;\n            padding: 0;\n            background: #fff;\n            color: #1a1a1a;\n            overflow-x: hidden;\n        }\n\n        \/* Timeline Container *\/\n        .timeline-container {\n            width: 90%;\n            max-width: 800px;\n            margin: 50px auto;\n            position: relative;\n            padding-top: 50px;\n        }\n\n        \/* Timeline Line *\/\n        .timeline-line {\n            position: absolute;\n            left: 50%;\n            width: 4px;\n            background: #0171EB;\n            height: 0;\n            top: 0;\n            transform: translateX(-50%);\n            z-index: -1;\n            transition: height 0.5s ease-out;\n        }\n\n        \/* Step Wrapper *\/\n        .step-wrapper {\n            display: flex;\n            align-items: center;\n            justify-content: center;\n            flex-direction: column;\n            position: relative;\n            opacity: 0;\n            transform: translateY(50px);\n            transition: opacity 0.6s ease-out, transform 0.6s ease-out;\n        }\n\n        \/* Step Box *\/\n        .step {\n            width: 100%;\n            max-width: 600px;\n            background: #ffffff;\n            border-radius: 12px;\n            padding: 20px;\n            margin: 40px auto;\n            position: relative;\n            box-shadow: 0px 0px 20px rgba(1, 113, 235, 0.2);\n            text-align: center;\n        }\n\n        \/* Step Content *\/\n        .step-number {\n            font-size: 1.2rem;\n            font-weight: bold;\n            color: #0171EB;\n        }\n\n        .step-title {\n            font-size: 1.5rem;\n            font-weight: bold;\n            margin-top: 5px;\n        }\n\n        .step-desc {\n            font-size: 1rem;\n            color: #555;\n        }\n\n        \/* Connecting Dots *\/\n        .step-dot {\n            width: 20px;\n            height: 20px;\n            background: #0171EB;\n            border-radius: 50%;\n            position: absolute;\n            left: 50%;\n            transform: translateX(-50%);\n            opacity: 0;\n            transition: opacity 0.3s ease-in-out;\n        }\n\n        \/* Position dots *\/\n        .step-wrapper:nth-child(1) .step-dot { top: -20px; }\n        .step-wrapper:nth-child(2) .step-dot { top: -20px; }\n        .step-wrapper:nth-child(3) .step-dot { top: -20px; }\n\n        \/* Glowing Effect *\/\n        .step-dot::after {\n            content: \"\";\n            width: 35px;\n            height: 35px;\n            background: rgba(1, 113, 235, 0.2);\n            position: absolute;\n            top: 50%;\n            left: 50%;\n            transform: translate(-50%, -50%);\n            border-radius: 50%;\n            animation: pulse 1.5s infinite ease-in-out;\n        }\n\n        @keyframes pulse {\n            0% { transform: translate(-50%, -50%) scale(1); opacity: 0.5; }\n            100% { transform: translate(-50%, -50%) scale(1.3); opacity: 0; }\n        }\n\n        \/* Individual Slide-in Animation *\/\n        .step-wrapper.active {\n            opacity: 1;\n            transform: translateY(0);\n        }\n\n        .step-wrapper:nth-child(1).active { transition-delay: 0s; }\n        .step-wrapper:nth-child(2).active { transition-delay: 0.3s; }\n        .step-wrapper:nth-child(3).active { transition-delay: 0.6s; }\n\n        .step-dot.active {\n            opacity: 1;\n        }\n\n    <\/style>\n<\/head>\n<body>\n\n    <div class=\"timeline-container\">\n        <div class=\"timeline-line\"><\/div>\n\n        <div class=\"step-wrapper\">\n            <div class=\"step-dot\"><\/div>\n            <div class=\"step\">\n                <div class=\"step-number\">Step 1<\/div>\n                <div class=\"step-title\">Design Your Battery<\/div>\n                <div class=\"step-desc\">Create your custom battery with unique branding, engraving, and color choices.<\/div>\n            <\/div>\n        <\/div>\n\n        <div class=\"step-wrapper\">\n            <div class=\"step-dot\"><\/div>\n            <div class=\"step\">\n                <div class=\"step-number\">Step 2<\/div>\n                <div class=\"step-title\">Print & Ship<\/div>\n                <div class=\"step-desc\">We print and apply your design onto the batteries and ship them directly to your address.<\/div>\n            <\/div>\n        <\/div>\n\n        <div class=\"step-wrapper\">\n            <div class=\"step-dot\"><\/div>\n            <div class=\"step\">\n                <div class=\"step-number\">Step 3<\/div>\n                <div class=\"step-title\">Unbox & Protect Your Tools<\/div>\n                <div class=\"step-desc\">Enjoy theft-proof tools and seamless power for your projects.<\/div>\n            <\/div>\n        <\/div>\n\n    <\/div>\n\n    <script>\n        document.addEventListener(\"DOMContentLoaded\", function () {\n            const steps = document.querySelectorAll('.step-wrapper');\n            const stepDots = document.querySelectorAll('.step-dot');\n            const timelineLine = document.querySelector('.timeline-line');\n\n            function checkScroll() {\n                let scrollPosition = window.innerHeight + window.scrollY;\n                let firstStepOffset = steps[0].offsetTop;\n\n                \/\/ Adjust timeline height dynamically\n                let maxHeight = Math.min(scrollPosition - firstStepOffset, document.body.scrollHeight);\n                timelineLine.style.height = maxHeight + \"px\";\n\n                steps.forEach((step, index) => {\n                    let stepOffset = step.offsetTop + step.clientHeight \/ 3;\n                    if (scrollPosition > stepOffset) {\n                        step.classList.add(\"active\");\n                        stepDots[index].classList.add(\"active\");\n                    }\n                });\n            }\n\n            window.addEventListener(\"scroll\", checkScroll);\n            checkScroll();\n        });\n    <\/script>\n\n<\/body>\n<\/html>","color_scheme":"","padding_top":40,"padding_bottom":52}}},"order":["custom_liquid_Bmjari","featured_collection","custom_liquid_yrEnJD"]}